{
  "redirects": [
    {
      "source": "/old-route",
      "destination": "/new-route",
      "permanent": true
    },
    {
      "source": "/docs",
      "destination": "https://example.com/documentation",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "/index.html"
    }
  ]
}
